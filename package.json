{
	"name": "@douganderson444/panzoom-node",
	"version": "1.2.4",
	"main": "index.js",
	"module": "index.js",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package",
		"publish:npm": "npm publish ./package --access public",
		"prepare": "ts-patch install -s",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@sveltejs/package": "^2.3.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.0",
		"ts-patch": "^2.1.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-transform-extensions": "^1.0.1",
		"vite": "^5.1.6"
	},
	"peerDependencies": {
		"svelte": "^4.2.12"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"type": "module",
	"keywords": [
		"svelte",
		"zoom",
		"panzoom"
	],
	"dependencies": {
		"pointer-tracker": "^2.5.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DougAnderson444/panzoom-node.git"
	},
	"readme": "https://github.com/DougAnderson444/panzoom-node",
	"author": "Doug Anderson"
}
